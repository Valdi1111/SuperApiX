  <build>
    <defaultGoal>clean package install</defaultGoal>
    <resources>
      <resource>
        <directory>src/main/resources</directory>
      </resource>
    </resources>
    <finalName>${project.artifactId} v${project.version}</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.4.3</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <minimizeJar>false</minimizeJar>
              <createDependencyReducedPom>true</createDependencyReducedPom>
              <relocations>
                <relocation>
                  <pattern>net.kyori.text</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.text</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.github.benmanes.caffeine</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.caffeine</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>okio</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.okio</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>okhttp3</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.okhttp3</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.h2database.h2</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.h2</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>org.mariadb.jdbc</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.mariadb</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.mongodb</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.mongodb</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.mysql</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.mysql</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>org.postgresql</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.postgresql</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>com.zaxxer.hikari</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.hikari</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>org.bson</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.bson</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>org.apache.commons.pool2</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.commonspool2</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>ninja.leaping.configurate</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.configurate</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>redis.clients.jedis</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.jedis</shadedPattern>
                </relocation>
                <relocation>
                  <pattern>redis.clients.util</pattern>
                  <shadedPattern>org.valdi.SuperApiX.lib.jedisutil</shadedPattern>
                </relocation>
              </relocations>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
